                    "input[name='zipcode']",
                    "input[name='address2']",
                    "input[name='telephone']",
                    "input[name='guess']",
                    "input[name='submitBack']"
                ]
            }]
        },
        {
            "name": "Geico",
            "url": "https://optout.geico.com/marketing/",
            "steps": [{
                "highlights": [
                    "#firstName",
                    "#lastName",
                    "#street",
                    "#street2",
                    "#city",
